{
  "display_name": "Apache Toree - Scala",
  "language": "scala",
  "argv": [
    "/opt/conda/share/jupyter/kernels/apache_toree_scala/bin/run.sh",
    "--profile",
    "{connection_file}"
  ],
  "env": {
    "DEFAULT_INTERPRETER": "Scala",
	"SPARK_OPTS": "--master=local[1] --driver-java-options=-Xms1024M --driver-java-options=-Xmx4096M --driver-java-options=-Dlog4j.logLevel=info --repositories http://repository.mapr.com/maven --packages org.apache.hbase:hbase-client:1.1.1-mapr-1602-m7-5.1.0,org.apache.hbase:hbase-server:1.1.1-mapr-1602-m7-5.1.0",
    "__TOREE_SPARK_OPTS__": "", 
    "__TOREE_OPTS__": "",
    "SPARK_HOME": "/usr/local/spark",
    "PYTHONPATH": "/usr/local/spark/python:/usr/local/spark/python/lib/py4j-0.10.4-src.zip",
    "PYTHON_EXEC": "python"
  }
}
